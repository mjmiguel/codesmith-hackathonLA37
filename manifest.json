{
  "name": "ToggleDoggle",
  "version": "1.0",
  "description": "adds iteresting features to websites",
  "permissions": ["storage"],
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["main.js", "popup.js"]
  }
],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "rickShibe.png"
  },
  "manifest_version": 2
}